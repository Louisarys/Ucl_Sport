CREATE TABLE favoris (id_User NOT NULL, id_Event NOT NULL);
CREATE TABLE Gps ( id_Gps NOT NULL, nom NOT NULL, lieu NOT NULL, latitude NOT NULL, longitude NOT NULL);
CREATE TABLE Planning (ID INTEGER PRIMARY KEY, SPORT STRING, SEXE STRING, LIEU STRING, SALLE STRING, JOUR STRING, DATE STRING, HEURE_DEBUT STRING, HEURE_FIN STRING, TYPE STRING, INSCRIPTION STRING, ACTIVE BOOLEAN);
CREATE TABLE RemarquesPlanning (ID INTEGER PRIMARY KEY, CORPS STRING, ORDRE INTEGER);
CREATE TABLE Users (ID PRIMARY KEY UNIQUE NOT NULL, Login UNIQUE NOT NULL, Password UNIQUE NOT NULL);
CREATE TABLE 'Update' (ID STRING PRIMARY KEY, VERSION INTEGER, NAME STRING, MAXID INTEGER);
INSERT INTO 'Update' (ID, VERSION, NAME, MAXID) VALUES ('disciplines', 1, 'Planning', 0);
INSERT INTO 'Update' (ID, VERSION, NAME, MAXID) VALUES ('favoris', 1, 'favoris', 0);
INSERT INTO 'Update' (ID, VERSION, NAME, MAXID) VALUES ('remarques', 1, 'RemarquesPlanning', 0);
INSERT INTO 'Update' (ID, VERSION, NAME, MAXID) VALUES ('update', 1, 'Update', 1);
INSERT INTO 'Update' (ID, VERSION, NAME, MAXID) VALUES ('gps', 1, 'Gps', 0);
CREATE TABLE planningbis (id INTEGER PRIMARY KEY AUTOINCREMENT, SPORT STRING, SEXE STRING, LIEU STRING, SALLE STRING, JOUR STRING, DATE DATE, HEUREDEBUT DATETIME, HEUREFIN DATETIME, TYPE STRING, INSCRIPTION STRING, REMARQUES STRING, ACTIVE BOOLEAN);